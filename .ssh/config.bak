Host *
 ServerAliveInterval 240

Host github.com
 Hostname ssh.github.com
 Port 443

Host bitbucket.org
 Hostname altssh.bitbucket.org
 Port 443

host *.llnl.gov
 ProxyCommand corkscrew snowman.ornl.gov 3128 %h %p

host *.anl.gov
 ProxyCommand corkscrew snowman.ornl.gov 3128 %h %p

host *.nersc.gov
 ProxyCommand corkscrew snowman.ornl.gov 3128 %h %p
 IdentityFile ~/.ssh/nersc
 IdentitiesOnly yes

host *.ucar.edu
 ProxyCommand corkscrew snowman.ornl.gov 3128 %h %p

host *.nasa.gov
 ProxyCommand corkscrew snowman.ornl.gov 3128 %h %p

host *.noaa.gov
 ProxyCommand corkscrew snowman.ornl.gov 3128 %h %p

host ice.gi.alaska.edu
 ProxyCommand corkscrew snowman.ornl.gov 3128 %h %p

host jhkennedy.org
 ProxyCommand corkscrew snowman.ornl.gov 3128 %h %p
